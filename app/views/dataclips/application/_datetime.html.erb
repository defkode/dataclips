<div class='input-group datetime' id='<%= id %>_from' rel="<%= key %>_from" >
  <input class="form-control datetime" name="<%= key %>_from" />
  <span class="input-group-addon">
    <i class="fa fa-calendar"></i>
  </span>
</div>

<div class='input-group datetime' id='<%= id %>_to' rel="<%= key %>_to">
  <input class="form-control datetime" name="<%= key %>_to" />
  <span class="input-group-addon">
    <i class="fa fa-calendar"></i>
  </span>
</div>

<script>
  $(document).ready(function(){
    var options = {
      sideBySide: true,
      locale: '<%= I18n.locale %>',
      format: "L HH:mm:ss",
      icons: {
        time:     "fa fa-clock-o",
        date:     "fa fa-calendar",
        next:     "fa fa-chevron-right",
        previous: "fa fa-chevron-left",
        up:       "fa fa-chevron-up",
        down:     "fa fa-chevron-down"
      }
    }

    var fromPicker = $('#<%= id %>_from').datetimepicker(options);
    var toPicker = $('#<%= id %>_to').datetimepicker(options);
  });
</script>
